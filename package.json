{
  "name": "ice-spear-editor",
  "version": "2.2.2",
  "license": "GPL-3.0",
  "author": "Max Bebök",
  "main": "main.js",
  "bin": {
    "ice-spear": "./main.js"
  },
  "scripts": {
    "start": "electron . --js-flags=\"--expose-gc\"",
    "update-license-csv": "license-checker --csv > MODULES_LICENSE.csv",
    "test": "electron-mocha --recursive",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "gitlab:ice-spear-tools/ice-spear"
  },
  "ice-spear": {
    "debug-mode": true,
    "default-app": "launcher",
    "app-icon": "assets/icons/icon_256.png",
    "default-size": [
      1500,
      820
    ]
  },
  "build": {
    "appId": "ice-spear-tools.ice-spear",
    "productName": "Ice-Spear",
    "copyright": "©2019 Max Bebök",
    "npmRebuild": true,
    "win": {
      "target": "7z"
    }
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "electron": "5.0.1",
    "electron-builder": "^20.22.0",
    "electron-mocha": "^5.0.0",
    "mocha": "^5.0.4"
  },
  "dependencies": {
    "ace-builds": "^1.3.3",
    "binary-file": "gitlab:ice-spear-tools/binary-file",
    "byaml-lib": "gitlab:ice-spear-tools/byaml-lib",
    "crc32": "^0.2.2",
    "crc-32": "^1.2.0",
    "cytoscape": "^3.2.15",
    "cytoscape-cose-bilkent": "^4.0.0",
    "dateformat": "^3.0.3",
    "delete": "^1.1.0",
    "draggable": "^4.2.0",
    "electron-remote": "^1.2.0",
    "filesize": "^3.6.1",
    "find-free-port": "^2.0.0",
    "fs-extra": "^5.0.0",
    "izitoast": "^1.3.0",
    "nanotimer": "^0.3.15",
    "pako": "^1.0.6",
    "picogl": "^0.8.9",
    "png-lib": "gitlab:ice-spear-tools/png-lib",
    "sanitize-filename": "^1.6.1",
    "sarc-lib": "gitlab:ice-spear-tools/sarc-lib",
    "split.js": "^1.3.5",
    "sweetalert2": "^7.19.2",
    "texture-lib": "gitlab:ice-spear-tools/texture-lib",
    "three-orbit-controls": "^82.1.0",
    "uuid": "^3.2.1",
    "yaz0-lib": "gitlab:ice-spear-tools/yaz0-lib"
  }
}
